<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.0">

	<!-- HIV ART Register Module Properties -->
	<id>@MODULE_ID@</id>
	<name>@MODULE_NAME@</name>
	<version>@MODULE_VERSION@</version>
	<package>@MODULE_PACKAGE@</package>
	<author>PIH</author>
	<description>Support module for reporting framework that provides ObjectGroup and dynamic calendar functionality</description>
	<activator>org.openmrs.module.reportingobjectgroup.ReportingObjectGroupModuleActivator</activator>
	<require_version>${openMRSVersion}</require_version>

	<require_modules>
		<require_module version="${reportingVersion}">org.openmrs.module.reporting</require_module>
	</require_modules>

	<aware_of_modules>
		<aware_of_module>org.openmrs.module.legacyui</aware_of_module>
	</aware_of_modules>

	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>

</module>
